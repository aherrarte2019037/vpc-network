client
dev tun
proto udp
remote 34.70.232.153 1194

resolv-retry infinite
nobind
persist-key
persist-tun

# Autenticación por usuario/contraseña
auth-user-pass

# Verificación del certificado del servidor
remote-cert-tls server

# Cifrado y autenticación (deben coincidir con server.conf)
cipher AES-256-CBC
auth SHA256

# TLS auth key (mismo que en el servidor, dirección 1)
key-direction 1

verb 3

